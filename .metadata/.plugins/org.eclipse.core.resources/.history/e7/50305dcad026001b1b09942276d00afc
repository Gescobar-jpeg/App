package myproject;
public class Application {

	public static void main(String[] args) {
	//
	/*
	 * primitive:
	 * int
	 * double
	 * float
	 * long
	 * short
	 * byte
	 * char
	 * boolean
	 * 
	 * Objects
	 * String - textual date a string of characters
	 */
	
	// variable identifier assigment operator, value semicolon
	int samsAge = 67;
	double accountBalance = 34.67;
	char middleInitial = 'C';
	boolean isHoutOutside = true;
	String firstName = "Sam";
	
	
	System.out.println(samsAge);
	System.out.println(accountBalance);
	System.out.println(middleInitial);
	System.out.println(isHoutOutside);
	System.out.println(firstName);
	}

}
